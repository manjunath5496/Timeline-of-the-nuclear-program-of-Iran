    \(S[a_ < b_, c_] := a + c < b + c\), "\[IndentingNewLine]", 
    \(S[a_ \[LessEqual] b_, c_] := 
      a + c \[LessEqual] b + c\), "\[IndentingNewLine]", 
    \(S[a_ > b_, c_] := a + c > b + c\), "\[IndentingNewLine]", 
    \(S[a_ \[GreaterEqual] b_, c_] := 
      a + c \[GreaterEqual] b + c\), "\[IndentingNewLine]", 
    \(S[a_ \[Equal] b_, c_] := 
      a\  + c \[Equal] \(\(b + c\)\(\[IndentingNewLine]\)
        \)\), "\[IndentingNewLine]", 
    \(M[a_ < b_, c_] := a\ c < b\ c\), "\[IndentingNewLine]", 
    \(M[a_ \[LessEqual] b_, c_] := 
      a\ c \[LessEqual] b\ c\), "\[IndentingNewLine]", 
    \(M[a_ > b_, c_] := a\ c > b\ c\), "\[IndentingNewLine]", 
