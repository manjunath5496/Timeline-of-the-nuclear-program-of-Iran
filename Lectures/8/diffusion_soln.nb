    \(Op[a_ < b_, c_] := c[a] < c[b]\), "\[IndentingNewLine]", 
    \(Op[a_ \[LessEqual] b_, c_] := 
      c[a] \[LessEqual] c[b]\), "\[IndentingNewLine]", 
