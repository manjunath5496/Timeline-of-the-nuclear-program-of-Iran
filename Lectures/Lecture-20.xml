          </mrow>
          <mo>+</mo>
          <mn>1</mn>
         </mrow>
         <mo>)</mo>
        </mrow>
        <mo>&InvisibleTimes;</mo>
        <mrow>
         <mi>y</mi>
         <mo>[</mo>
         <mi>x</mi>
         <mo>]</mo>
        </mrow>
       </mrow>
      </mrow>
      <mtext> </mtext>
      <mo>&Equal;</mo>
      <mn>0</mn>
     </mrow>
     <mo>,</mo>
     <mrow>
      <mrow>
       <mi>y</mi>
       <mo>[</mo>
       <mn>0</mn>
       <mo>]</mo>
      </mrow>
      <mo>==</mo>
      <mn>4</mn>
